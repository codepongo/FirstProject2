<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
		 width="100%" height="100%"
		 viewBox="0 0 600 400">

	<script>

		function down(evt)
		{
		evt.target.cx.baseVal.value = evt.target.cx.baseVal.value - 10;
		}

	</script>
	
	<filter id="Blur" x="-10%" y="-10%" width="120%" height="120%">
		<feGaussianBlur in="SourceGraphic" stdDeviation="12" />
	</filter>

	<image xlink:href="iron_man_5.bmp" preserveAspectRatio="none" x="20" y="20" width="300" height="200" style="opacity: 1.0" />

	<ellipse onmousedown="down(event);" cx="130" cy="90" rx="100" ry="60" style="fill:rgb(255, 0,0); stroke:rgb(255, 255, 0); stroke-width: 8px"/>

	<line x1="20" y1="30" x2="140" y2="190" style="stroke:rgb(0, 255, 0); stroke-width: 15px" />

	<rect x="50" y="60" width="100" height="80" rx="20" ry="20" style="fill:rgb(90, 255, 255)" />

	<polygon points="10 10, 100 20, 70 210" style="fill: rgb(80, 80, 80); stroke:rgb(255, 0, 0); stroke-width: 10" />

</svg>
